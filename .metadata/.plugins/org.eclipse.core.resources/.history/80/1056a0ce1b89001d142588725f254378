package Strings_Arrays;
import java.util.Arrays;

public class Arrayex {

	public static void main(String[] args) {
		int[] arr= {6,2,8,1,3};
		int[] arr2= {1,2,3,4};
		String[] strArr=new String[3];
		strArr[0]="Cat";
		strArr[1]="Water";
		strArr[2]="Sheep";
		
		Arrays.sort(arr);
		System.out.println("Sorted Array: "+ arr.toString());
		
		System.out.println("Equal or not:" +Arrays.equals(arr,arr2));
		
		Arrays.fill(arr,99);
		System.out.println("New Array: "+ arr.toString());
		

	}

}
